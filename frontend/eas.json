{
  "cli": {
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "environment": "development",
      "android": {
        "image": "ubuntu-24.04-android-35"
      }
    },

    "preview": {
      "android": {
        "buildType": "apk",
        "image": "ubuntu-24.04-android-35"
      },
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "android": {
        "credentialsSource": "local",
        "image": "ubuntu-24.04-android-35"
      },
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2"
      },
      "environment": "production",
      "channel": "production",
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
